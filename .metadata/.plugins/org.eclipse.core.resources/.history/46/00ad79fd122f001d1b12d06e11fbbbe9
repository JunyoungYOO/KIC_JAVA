package chap11;

public class StringEx2 {

	public static void main(String[] args) {
		String str = "ABC123";
		System.out.println("main change Àü : "+ str);
		change(str);
		System.out.println("main change ÈÄ : "+ str);
	}
	
	private static void change(String str) {
		str += "456";
		System.out.println("change : "+str);
	}

}
